        "@typescript-eslint/parser": "^5.56.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "cross-fetch": "^3.1.5",
        "eslint": "^8.36.0",
        "isomorphic-ws": "^5.0.0",
        "jest": "^29.5.0",
        "jest-config": "^29.5.0",
        "react": "^18.2.0",
        "rollup": "^3.20.2",
        "rollup-plugin-terser": "^7.0.2",
        "rollup-plugin-typescript2": "^0.34.1",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "tslib": "^2.5.0",
        "ttypescript": "^1.5.15",
        "typescript": "^5.0.2",
